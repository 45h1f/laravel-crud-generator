{
    "name": "ashiful/crud",
    "description": "Easy way to generate crud",
    "keywords": [
        "ashiful",
        "crud"
    ],
    "homepage": "https://github.com/ashif007/laravel-crud-generator",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ashiful Islam",
            "email": "ashiful134@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "laravel/prompts": "^0.1.6",
        "spatie/laravel-html": "^3.2",
        "yajra/laravel-datatables": "10.0"
    },
    "autoload": {
        "psr-4": {
            "Ashiful\\Crud\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ashiful\\Crud\\CrudServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
